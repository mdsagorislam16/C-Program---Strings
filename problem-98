#include <stdio.h>
#include <string.h>

int main() {
    char s[500];
    fgets(s, sizeof(s), stdin);

    int i, freq[26] = {0};

    for (i = 0; s[i] != '\0'; i++) {
        char ch = s[i];
        if (ch >= 'A' && ch <= 'Z') ch += 32; // lowercase
        if (ch >= 'a' && ch <= 'z')
            freq[ch - 'a'] = 1;
    }

    int pangram = 1;
    for (i = 0; i < 26; i++) {
        if (freq[i] == 0) {
            pangram = 0;
            break;
        }
    }

    if (pangram) printf("Pangram\n");
    else printf("Not Pangram\n");

    return 0;
}

/*
Input:
The quick brown fox jumps over the lazy dog

Output:
Pangram
*/

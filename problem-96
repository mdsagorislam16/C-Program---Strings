#include <stdio.h>

int main() {
    char s[200];
    fgets(s, sizeof(s), stdin);

    int i, cnt = 0;
    for (i = 0; s[i] != '\0'; i++) {
        cnt++;
    }

    // Remove newline from count if exists
    if (s[cnt-1] == '\n') cnt--;

    printf("Length = %d", cnt);
    return 0;
}

/*
Input:
hello world

Output:
Length = 11
*/

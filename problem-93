#include <stdio.h>
#include <string.h>

int main() {
    char s[200];
    fgets(s, sizeof(s), stdin);

    s[strcspn(s, "\n")] = '\0';

    int l = 0, r = strlen(s) - 1;
    int ok = 1;

    while (l < r) {
        if (s[l] != s[r]) {
            ok = 0;
            break;
        }
        l++;
        r--;
    }

    if (ok) printf("Palindrome\n");
    else printf("Not Palindrome\n");

    return 0;
}

/*
Input:
madam

Output:
Palindrome
*/

#include <stdio.h>

int main() {
    char s[200];
    fgets(s, sizeof(s), stdin);

    int i;
    if (s[0] != ' ' && s[0] != '\n') printf("%c ", s[0]);

    for (i = 1; s[i] != '\0'; i++) {
        if (s[i-1] == ' ' && s[i] != ' ' && s[i] != '\n') {
            printf("%c ", s[i]);
        }
    }

    return 0;
}

/*
Input:
Hello world from C

Output:
H w f C
*/

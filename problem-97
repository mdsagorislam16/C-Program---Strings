#include <stdio.h>

int main() {
    char s[200];
    fgets(s, sizeof(s), stdin);

    int i, j, n;
    // Remove newline if exists
    for (n = 0; s[n] != '\0'; n++);
    if (s[n-1] == '\n') s[--n] = '\0';

    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (s[i] > s[j]) {
                char temp = s[i];
                s[i] = s[j];
                s[j] = temp;
            }
        }
    }

    printf("Sorted string: %s\n", s);
    return 0;
}

/*
Input:
programming

Output:
aggimmnoprr
*/

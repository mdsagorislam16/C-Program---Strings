#include <stdio.h>
#include <string.h>

void rev(char s[], int l, int r) {
    if (l >= r) return;

    char temp = s[l];
    s[l] = s[r];
    s[r] = temp;

    rev(s, l + 1, r - 1);
}

int main() {
    char s[200];
    fgets(s, sizeof(s), stdin);

    s[strcspn(s, "\n")] = '\0';

    rev(s, 0, strlen(s) - 1);

    printf("Reversed: %s\n", s);

    return 0;
}

/*
Input:
hello

Output:
Reversed: olleh
*/

#include <stdio.h>
#include <string.h>

int main() {
    char s[200];
    fgets(s, sizeof(s), stdin);

    int i = 0, n = strlen(s);

    // remove newline
    if (s[n-1] == '\n') s[--n] = '\0';

    while (s[i] == '0') i++;

    if (i == n) printf("0\n"); // all zeros
    else printf("%s\n", s + i);

    return 0;
}

/*
Input:
00012345

Output:
12345
*/

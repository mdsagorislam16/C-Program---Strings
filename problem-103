#include <stdio.h>

int main() {
    char a[200], b[200];
    fgets(a, sizeof(a), stdin);
    fgets(b, sizeof(b), stdin);

    int i;
    int n = strlen(a); if (a[n-1]=='\n') a[n-1]='\0';
    n = strlen(b); if (b[n-1]=='\n') b[n-1]='\0';

    for (i=0; a[i]!='\0' && b[i]!='\0'; i++) {
        if (a[i]<b[i]) { printf("a < b\n"); return 0; }
        else if (a[i]>b[i]) { printf("a > b\n"); return 0; }
    }

    if (a[i]=='\0' && b[i]=='\0') printf("a = b\n");
    else if (a[i]=='\0') printf("a < b\n");
    else printf("a > b\n");

    return 0;
}

/*
Input:
apple
apricot

Output:
a < b
*/

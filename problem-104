#include <stdio.h>
#include <string.h>

int main() {
    char s1[200], s2[200], res[400];
    int pos;
    fgets(s1, sizeof(s1), stdin);
    fgets(s2, sizeof(s2), stdin);
    scanf("%d", &pos);

    // remove newline
    s1[strcspn(s1, "\n")] = '\0';
    s2[strcspn(s2, "\n")] = '\0';

    int i, j = 0;
    for (i=0; i<pos; i++) res[j++] = s1[i];
    for (i=0; s2[i]!='\0'; i++) res[j++] = s2[i];
    for (i=pos; s1[i]!='\0'; i++) res[j++] = s1[i];
    res[j] = '\0';

    printf("%s\n", res);

    return 0;
}

/*
Input:
HelloWorld
C
5

Output:
HelloCWorld
*/
